{
    "workbench.colorCustomizations": {
        // "tab.activeBackground": "#ff0000" // tabs at the top
        // "activityBar.background": "#ff0000"
        "sideBar.background": "#171616", // the extensions side
        // "tab.activeBorder": "#ff0000", // tabs ke niche line
        "editor.background": "#171616", // editor screen ka bg

        "sideBarSectionHeader.background": "#171616",
        "titleBar.activeBackground": "#171616",
        "tab.activeBackground": "#1c1c1c",
        "tab.inactiveBackground": "#171616",
        "tab.unfocusedActiveBackground": "#171616",
        "editorGroupHeader.tabsBackground": "#171616",
        "terminal.background": "#171616",
    },

    "workbench.startupEditor": "none",
    "editor.fontFamily": "'JetBrains Mono NL'",
    "editor.minimap.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.renderWhitespace": "none",
    "workbench.statusBar.visible": false,
    "code-runner.runInTerminal": true,
    "terminal.integrated.fontWeightBold": "normal",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1,
    "terminal.integrated.fontSize": 16,
    "C_Cpp.default.compilerPath": "/opt/homebrew/opt/llvm/bin/clang++",
    "code-runner.executorMap": {

        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "zig": "zig run",
        "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        "python": "python -u",
        "perl": "perl",
        "perl6": "perl6",
        "ruby": "ruby",
        "go": "go run",
        "lua": "lua",
        "groovy": "groovy",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "fsharp": "fsi",
        "csharp": "scriptcs",
        "vbscript": "cscript //Nologo",
        "typescript": "ts-node",
        "coffeescript": "coffee",
        "scala": "scala",
        "swift": "swift",
        "julia": "julia",
        "crystal": "crystal",
        "ocaml": "ocaml",
        "r": "Rscript",
        "applescript": "osascript",
        "clojure": "lein exec",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "racket",
        "scheme": "csi -script",
        "ahk": "autohotkey",
        "autoit": "autoit3",
        "dart": "dart",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "runghc",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "lisp": "sbcl --script",
        "kit": "kitc --run",
        "v": "v run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "cd $dir && sml $fileName"
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "leetcode.defaultLanguage": "cpp",
    "leetcode.workspaceFolder": "/Users/akshatsharma/Developer/Programming/LeetCode",
    "editor.wordWrap": "on",
    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.hint.commentDescription": false,
    "leetcode.hint.commandShortcut": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "html.format.wrapAttributes": "force-aligned",
    "editor.detectIndentation": true,
    "editor.fontWeight": "500",
    "workbench.iconTheme": "bearded-icons",
    "git.autofetch": true,
    "breadcrumbs.enabled": false,
    "editor.renderControlCharacters": false,
    "terminal.integrated.fontFamily": "'Iosevka Nerd Font'",
    "workbench.activityBar.location": "top",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "http.proxySupport": "on",
    "discord.lowerDetailsIdling": "LazyPaloozing  ",
    "discord.removeTimestamp": true,
    "discord.largeImageIdling": "LazyPaloozing",
    "workbench.tree.indent": 23,
    "files.trimTrailingWhitespace": true,
    "editor.fontLigatures": true,
    "files.simpleDialog.enable": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    // "workbench.preferredHighContrastLightColorTheme": "Yume Dark",
    "workbench.layoutControl.enabled": false,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": "active",
    "editor.accessibilitySupport": "off",
    "workbench.colorTheme": "One Dark Pro Flat",
    "editor.fontSize": 16,
    "terminal.integrated.fontWeight": "normal",
    "editor.renderLineHighlight": "none",
    "git.decorations.enabled":false,
    "vscord.app.id": "CodiPodi",
    "vscord.app.name": "Visual Studio Code",

}
